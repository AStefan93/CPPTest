{
    "files.exclude": {
        "**/bazel*": true
    },
    "files.associations": {
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "algorithm": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "numeric": "cpp",
        "optional": "cpp",
        "random": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "fstream": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "typeinfo": "cpp"
    },
    "testMate.cpp.test.executables": "{build,Build,BUILD,out,Out,OUT,bazel-out/k8-fastbuild/bin/src}/**/__main__/**/*{test,Test,TEST}*",
    "clangd.onConfigChanged": "restart",
    "bazel.buildifierFixOnFormat": true,
    "bazel.enableCodeLens": true,
    "terminal.integrated.defaultProfile.linux": "bash",
    "editor.defaultFormatter": "xaver.clang-format",
    "editor.formatOnSave": true,
    "C_Cpp.codeAnalysis.clangTidy.checks.enabled": [
        "*"
    ],
    "git.allowForcePush": true,
    "doxdocgen.generic.dateTemplate": "",
    "doxdocgen.file.copyrightTag": [],
    "doxdocgen.file.versionTag": "",
    "doxdocgen.generic.useGitUserEmail": true,
    "doxdocgen.generic.useGitUserName": true,
    "clangd.checkUpdates": true,
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "workbench.editor.showTabs": true,
}